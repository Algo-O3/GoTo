{
    "version": 2,
    "name": "goto-server",

    "builds": [
        { "src": "server.js", "use": "@vercel/node" }
     ],
     "routes": [
        { "src": "/.*",
          "dest": "server.js",
          "headers": {
            "Access-Control-Allow-Origin": "https://goto-nine.vercel.app"
          }
        }
    ],
    "crons": [
        {
          "path": "/travelDetails/dailyCleanUp",
          "schedule": "32 18 * * *"
        }
    ]
}